<p style="font-size: 40px; font: bold;"><%= @member.name %></p>

<div class="container">
  <img style="width= 50px; height= 100px;"class="img-rounded" <%= image_tag @member.image_url.to_s %>
</div>
<br>
<ul>
  <li> <strong>Mother :-</strong> <%= @member.mother %></li><br>
  <li> <strong>Father :-</strong> <%= @member.father %></li><br>
  <li> <strong>Sibling :-</strong> <%= @member.email %></li><br>
  <li> <strong>Married_to :-</strong> <%= @member.married_to %></li><br>
  <li> <strong>Children :-</strong> <%= @member.children %></li><br>
  <li> <strong>Social Link</strong> <%= @member.social_media %></li><br>
  <li> <strong>Date of Birth</strong> <%= @member.date_of_birth %></li><br>
  <li> <strong>Place of Birth</strong> <%= @member.place_of_birth %></li><br>
  <li> <strong>Job :-</strong> <%= @member.job %></li><br>
  <li> <strong>Email :-</strong> <%= @member.email %></li><br>
  <li> <strong>Telephone :-</strong> <%= @member.telephone %></li><br>
  <li> <strong>Location</strong> <%= @member.location %></li>
  <li> <strong>Background :-</strong> <%= @member.context %></li><br>
</ul>
